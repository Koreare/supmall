.header{
  background-image: linear-gradient(#62A5FF, #29C0D9);
}
.img{
  height: 130rpx;
  width: 130rpx;
  margin-left: 35rpx;
}
.img image{
  height: 100%;
  width: 100%;
  border-radius: 50%;
}
.header_info_image{
  display: flex;
  align-items: center;
}
.name{
  margin-left: 20rpx;
}
.name_info{
  font-size: 38rpx;
  color:rgba(255,255,255,1);
  margin-bottom: 15rpx;
}
.day{
  font-size: 25rpx;
  display: flex;
  align-items: center;
  color:rgba(255,255,255,1);
}
.header_info{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 280rpx;
}
.check{
  width: 150rpx;
  height:55rpx;
  line-height:55rpx;
  text-align: center;
  color:rgba(68,187,255,1);
  background-color:rgba(255,255,255,1);
  border-radius: 20rpx; 
  font-size: 32rpx;
  margin-right: 35rpx;
  margin-top: -50rpx;
}
.width{
  display: flex;
}
.width_info{
  margin-top: 40rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}


.height{
  width: 70rpx;
  margin-left: 10rpx;
}
.height_1{
  color:rgba(253,254,255,1);
  font-size: 30rpx;
}
.height_3{
  color:rgba(253,254,255,1);
  font-size: 30rpx;
}

.height_2{
  margin:10rpx 0 10rpx 15rpx;
  height: 15rpx;
  width: 15rpx;
  border-radius: 50%;
  background-color: rgba(255,255,255,1);
}
.first::after{
  content: " "!important;
	display: block !important;
  width: 55rpx !important;
  height: 8rpx !important;
  background-color:#dcdcdc !important;
  position: relative !important;
  top: 4rpx !important;
  left: 15rpx !important;
}
.height_2::after{
  content: "";
	display: block;
  width: 70rpx;
  height: 8rpx;
  background-color:rgba(255,255,255,1);
  position: relative;
  top: 4rpx;
}
.height_first{
  width: 70rpx;
}
.point{
  text-align: center;
  color:rgba(253,254,255,1);
  font-size: 36rpx;
  margin-right: 45rpx;
}
.point view{
  margin-bottom: 15rpx;
}
.recording_info image{
  height: 60rpx;
  width: 55rpx;
  margin-right: 15rpx;
}
.recording_info view{
  font-size: 32rpx;
  color:rgba(102,102,102,1);
}
.right{
  width: 30rpx;
  height: 30rpx;
  margin-right: 30rpx;
}
.recording_info{
  display: flex;
  align-items: center;
  margin-left: 30rpx;
}
.recording{
  height: 110rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 5rpx solid #f5f5f5;
}
.bg{
  padding-top: 34rpx;
  margin-bottom: 30rpx;
  background-color: #f5f5f5;
  padding-bottom: 34rpx;
}
.good{
  width: 90%;
  height: 95%;
  margin-left: 5%;
  border-radius: 20rpx;
  border: 5rpx solid #D3D3D3;
  /* margin-top: 50rpx;   */
}
.good_top{
  height: 100rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  border-bottom: 5rpx solid#D3D3D3;
}
.good_top_name{
  font-size: 38rpx;
  color:rgba(153,153,153,1);
  border-right: 5rpx solid #D3D3D3;
  padding-right: 120rpx;
}
.good_top_name1{
  font-size: 35rpx;
  color:rgba(153,153,153,1);
}
.avtive{
  color:rgba(68,187,255,1);
}
.info{
  display: flex;
  flex-wrap: wrap;
  margin-left: 4%;
  margin-bottom: 30rpx;
}
.info_a{
  margin-top: 30rpx;
  width: 45%;
  border-radius: 20rpx; 
  border: 5rpx solid #dcdcdc;
  margin-right: 3%;
}
.info_image{
  width: 100%;
  height: 300rpx;
}
.info_image image{
  border-radius: 20rpx 20rpx 0 0; 
  height: 100%;
  width: 100%;
}
.info_name{
  font-size: 30rpx;
  color:rgba(51,51,51,1);
  margin-left: 15rpx;
  margin-bottom: 20rpx;
  overflow-x:hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.info_num{
  font-size: 30rpx;
  color:rgba(255,0,0,1);
  margin-left: 15rpx;
}
.info_n{
  width: 120rpx;
  height: 40rpx;
  text-align: center;
  line-height: 40rpx;
  background-image: linear-gradient(#62A5FF, #29C0D9);
  border-radius: 14rpx;
  color:rgba(255,255,255,1);
  font-size: 25rpx;
  margin: 20rpx 0 10rpx 10rpx;
}
.footer_info image{
  width: 350rpx;
  height: 540rpx;
  position: absolute;
  bottom: 18%;
  left: 26%;
}




