.header{
  height: 80rpx;
  background-image: linear-gradient(#62A5FF, #29C0D9);
}
.assess_image{
  display: flex;
  margin-top: 30rpx;
  margin-left: 30rpx;
}
.assess_image_image{
  height: 100rpx;
  width: 100rpx;
}
.assess_image_image image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.assess_image view{
  font-size: 32rpx;
  color:rgba(51,51,51,1);
  font-weight:500;
}
.name_2{
  margin-left: 30rpx;
  margin-top: 15rpx;
}
.assess_info{
 margin-left: 20%;
 color:rgba(102,102,102,1);
 font-size: 34rpx;
 width: 74%;
 
}
.assess_info view{
  margin-bottom: 60rpx;
  overflow-x:hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.assess_img{
  margin-top: 30rpx;
  display: flex;
  margin-left: 2%;
  flex-wrap: wrap;
  margin-bottom: 40rpx;
}
.assess_img_img{
  width: 30%;
  margin-right: 3%;
  margin-top: 15rpx;
}
.assess_img_img image{
  width: 100%;
  height: 100%;
  border-radius: 14rpx;
}
.assess{
  border-bottom: 5rpx solid #F5F5F5;
}
.button{
  display: flex;
  align-items: center;
  justify-content: flex-end;
  margin-bottom: 30rpx;
}
.button_1{
  width: 160rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  background-image: linear-gradient(#62A5FF, #29C0D9);
  color: #ffffff;
  border-radius: 14rpx;
  font-size: 30rpx;
  margin-right: 50rpx;
}
.button1{
  display: flex;
  justify-content: flex-end;
  margin-bottom: 15rpx;
}
.time{
  display: flex;
  justify-content: flex-end;
  align-items: center;
  margin: 10px;
  font-size: 30rpx;
  color:rgba(102,102,102,1);
}








