.header{
  height: 80rpx;
  background-image: linear-gradient(#62A5FF, #29C0D9);
}

.top{
  height: 90rpx;
  background-color: #f5f5f5;
  display: flex;
  align-items: center;
  font-size: 31rpx;
  color: rgba(102,102,102,1);
  padding-left: 30rpx;
}
.in_info_data{
  display: flex;
  justify-content: space-between;
  height: 116rpx;
  align-items: center;
  border-bottom: 5rpx solid #f5f5f5;
}
.in_info_data image{
  height: 35rpx;
  width: 30rpx;
  padding-right: 30rpx;
}
.in_info_name{
  color: rgba(102,102,102,1);
  font-size: 34rpx;
  padding-left: 30rpx;
}
.footer{
  width: 100%;
  text-align: center;
}
.footer_info{
  margin-top: 70rpx;
  text-align: center;
}
.footer_info image{
  height: 240rpx;
  width: 250rpx;
}
.footer_info_name{
  font-size: 32rpx;
  color: rgba(102,102,102,1);
}
.service{
  height: 90rpx;
  background-color: #f5f5f5;
  display: flex;
  align-items: center;
  font-size: 31rpx;
  color: rgba(102,102,102,1);
  padding-left: 30rpx;
}
contact-button{
  margin-left: 55rpx;
  padding-top: 20rpx;
  text-align: center;
}
.cBtn view{
  font-size: 29rpx;
  color: rgba(102,102,102,1);
  margin-top: 20rpx;
}