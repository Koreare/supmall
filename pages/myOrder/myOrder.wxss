.header{
  height: 80rpx;
  background-image: linear-gradient(#62A5FF, #29C0D9);
}
.top{
  width: 100%;
}
.top_info{
  width: auto;
  overflow-x: auto;
  white-space: nowrap;
  border-bottom: 5rpx solid #f5f5f5;
}
.top_info::-webkit-scrollbar {display:none}
.float{
  display: inline-block;
  height: 95rpx;
  line-height: 95rpx;
  margin-right: 12rpx;
  font-size: 32rpx;
  color: rgba(102,102,102,1);
  margin-left: 19rpx;
  text-align: center;
  width: 16%;
}
.active{
  color:rgba(68,187,255,1);
}
.info{
  margin-top: 30rpx;
}
.per{
  height: 40rpx;
  width: 40rpx;
  margin-left: 15rpx;
}
.right{
  margin-top: 8rpx;
  height: 30rpx;
  width: 30rpx;
}
.info_top{
  display: flex;
  align-items: center;
}
.info_top view{
  font-size: 32rpx;
  margin-right: 10rpx;
  margin-left: 10rpx;
}
.shop{
  width: 95%;
  margin-left: 2.5%;
  height: 220rpx;
  background-color: #f5f5f5;
  border-radius: 20rpx;
  margin-top: 30rpx;
}
.shop_info{
  display: flex;
  align-items: center;
  height: 220rpx;
}
.shop_name{
  width: 66%;
}
.shop_num{
  display: flex;
  justify-content: space-between;
}

.name{
  font-size: 33rpx;
  color:rgba(51,51,51,1);
  margin-bottom: 30rpx;
  overflow-x:hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.day{
  font-size: 30rpx;
}
.shop_number{
  display: flex;
}
.number{
  font-size: 30rpx;
}
.less{
  font-size: 30rpx;
  margin-top: 5rpx;
  margin-right: 8rpx;
}
.info{
  color:rgba(102,102,102,1);
  font-size: 34rpx;
  font-weight:400;
}
.img{
  margin-right: 15rpx;
  margin-left: 30rpx;
  border-radius: 20rpx;
  width: 180rpx;
  height: 150rpx;
}
.img image{
  border-radius: 20rpx;
  height: 100%;
  width: 100%;
}
.pay{
   height: 120rpx;
   display: flex;
   justify-content: flex-end;
   align-items: center;
   border-bottom: 5rpx solid #f5f5f5;
   font-size: 38rpx;
   font-weight:500;
   color:rgba(51,51,51,1);
   margin-right: 2.5%;
}
.button{
  display: flex;
  justify-content: flex-end;
  height: 120rpx;
  align-items: center;
  border-bottom: 15rpx solid #f5f5f5;
}
.button_1{
  width: 150rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  font-size: 30rpx;
  border: 5rpx solid #dcdcdc;
  border-radius: 14rpx;
  color:rgba(51,51,51,1);
  margin-right: 10rpx;
}
.button_2{
  width: 150rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  font-size: 30rpx;
  border: 5rpx solid #f5f5f5;
  border-radius: 14rpx;
  color:rgba(255,255,255,1);
  background-image: linear-gradient(#62A5FF, #29C0D9);
  margin-right: 20rpx;
}
.button_3{
  width: 150rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  font-size: 30rpx;
  border: 5rpx solid #dcdcdc;
  border-radius: 14rpx;
  color:rgba(51,51,51,1);
  margin-right: 10rpx;
}
.mark{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  background-color: #000000;
  opacity: 0.5;
}
.yes{
  width: 70%;
  height: 300rpx;
  margin-left: 15%;
  border-radius: 20rpx;
  position: fixed;
  top: 30%;
  background-color: #ffffff;
}
.yes_name{
  font-size: 35rpx;
  color:rgba(102,102,102,1);
  text-align: center;
  margin: 50rpx 30rpx 80rpx 30rpx;
}
.yes_info{
  display: flex;
  justify-content: space-around;
}
.yes_info view{
  width: 180rpx;
  height: 70rpx;
  text-align: center;
  line-height: 70rpx;
  background-image: linear-gradient(#62A5FF, #29C0D9);
  font-size: 32rpx;
  color:rgba(255,255,255,1);
  border-radius: 14rpx;
}
.yes_info text{
  width: 180rpx;
  height: 70rpx;
  text-align: center;
  line-height: 70rpx;
  background-color: #f5f5f5;
  font-size: 32rpx;
  color:rgba(153,153,153,1);
  border-radius: 14rpx;
}
.none_info{
  width: 70%;
  margin-left: 15%;
  border-radius: 20rpx;
  position: fixed;
  top: 30%;
  background-color: #ffffff;
}
.none_top{
  text-align: center;
  font-size: 32rpx;
  color: #D31008;
  padding: 15rpx 0;
}
.logistics{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 15rpx;
  margin: 40rpx 0;
}
.logistics_name{
  font-size: 30rpx;
  margin-right: 10rpx;
}
.delivery{
  display: flex;
  align-items: center;
  padding: 0 15rpx;
  margin-bottom: 20rpx;
}
.delivery_name{
  margin-right: 20rpx;
}
.delivery_type{
  color:rgba(102,102,102,1);
  width: 70%;
}
.all{
  position: relative;
  top: -16rpx;
  left: 31%;
  color:rgba(102,102,102,1);
}
.last{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 80rpx;
}
.last_left{
  width: 50%;
  height: 89rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 34rpx;
  /* border-radius: 0 0 14rpx 0; */
  background-color: #ffffff;
  border-radius: 0rpx;
}
.last_right{
  width: 50%;
  height: 89rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 34rpx;
  /* border-radius: 0 0 14rpx 0; */
  background-color: #ffffff;
  border-radius: 0rpx;
}
button{
  border-radius: 0rpx;
}
.select_box{
  background: #fff;
  width: 67%;
  margin: 30rpx auto;
  position: relative;
}
.select{
  box-sizing: border-box;
  width: 100%;
  height: 70rpx;
  border:1px solid #efefef;
  border-radius: 8rpx;
  display: flex;
  align-items: center;
  padding: 0 20rpx;
}
.select_text{
  font-size: 30rpx;
  flex: 1;
}
.select_img{
  width: 40rpx;
  height: 40rpx;
  display: block;
  transition:transform 0.3s;
}
.select_img_rotate{
  transform:rotate(180deg); 
}
.option_box{
  position: absolute;
  top: 70rpx;
  width: 100%;
  border:1px solid #efefef;
  box-sizing: border-box;
  height: 0;
  overflow-y: auto;
  border-top: 0;
  background: #fff;
  transition: height 0.3s;
}
.option{
  display: block;
  line-height: 40rpx;
  font-size: 30rpx;
  border-bottom: 1px solid #efefef;
  padding: 10rpx;
}