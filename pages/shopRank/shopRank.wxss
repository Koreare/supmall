.image{
  width: 100%;
  height: 300rpx;
  margin-bottom: 20rpx;
}
.image image{
  width: 100%;
  height: 100%;
}
.img{
  display: flex;
  align-items: center;
  margin-left: 20rpx;
}
.img view{
  font-size: 35rpx;
}
.refresh{
  height: 130rpx;
  border-top: 3rpx solid	#DCDCDC;
  border-bottom: 3rpx solid	#DCDCDC;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.c2{
  height: 60rpx;
  width: 60rpx;
  margin-right: 20rpx;
}
.right{
  height: 40rpx;
  width: 40rpx;
  margin-right: 20rpx;
}
.info_image{
  width: 200rpx;
  height: 200rpx;
  margin-right: 20rpx;
}
.info_image image{
  border-radius: 20rpx;
  width: 100%;
  height: 100%;
}
.info_address{
  display: flex;
  margin-bottom: 60rpx;
}
.info_address image{
  height: 40rpx;
  width: 35rpx;
  margin-top: 4rpx;
}
.info_name{
  position: absolute;
  top: 0rpx;
  left: 230rpx;
}
.info_number{
  display: flex;
}
.info_number image{
  height: 40rpx;
  width: 40rpx;
}
.info{
  margin-left: 20rpx;
  margin-top: 30rpx;
  width: 100%;
  position: relative;
}
.name{
  font-size: 35rpx;
  margin-bottom: 15rpx;
  overflow-x:hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.info_name{
  width: 66%;
}
.info_address view{
  width: 90%;
  color:rgba(205,205,205,1);
  font-size: 32rpx;
  margin-left: 15rpx;
  overflow-x:hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.money{
  display: flex;
  align-items: center;
  font-size: 25rpx;
  color:rgba(102,102,102,1);
}
